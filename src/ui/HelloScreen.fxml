        <Button fx:id="buttonStartTest" layoutX="185.0" layoutY="38.0" mnemonicParsing="false"
                onAction="#startTestClick" prefHeight="73.0" prefWidth="195.0" text="Start Test"/>
        <TextArea fx:id="textAreaNickname" layoutX="189.0" layoutY="258.0" pickOnBounds="false" prefHeight="37.0"
                  prefWidth="174.0"/>
        <Button fx:id="buttonCheckResult" layoutX="185.0" layoutY="134.0" mnemonicParsing="false" prefHeight="73.0"
                prefWidth="195.0" text="Check your result"/>
        <Label layoutX="191.0" layoutY="232.0" prefHeight="17.0" prefWidth="154.0" text="You nickname: "/>
    </AnchorPane>
</Group>

