<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="400.0" prefWidth="600.0"
            xmlns="http://javafx.com/javafx/10.0.2-internal" fx:controller="ui.HelloScreenController"
>
    <VBox alignment="CENTER" prefWidth="600" prefHeight="400">
        <HBox alignment="CENTER" prefWidth="600" prefHeight="400" spacing="10">
            <Button onAction="#onButtonEnterClick" text="Enter" minHeight="50" minWidth="100"/>
            <Button onAction="#onButtonRegisterClick" text="Register" minHeight="50" minWidth="100"/>
            <Button onAction="#onButtonLeaderboardClick" text="Leaderboard" minHeight="50" minWidth="100"/>
        </HBox>
    </VBox>
</AnchorPane>
