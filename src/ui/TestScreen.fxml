<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.Group?>
<?import javafx.scene.layout.AnchorPane?>
<Group xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/10.0.2-internal"
       fx:controller="ui.TestScreenController">
    <AnchorPane prefHeight="500.0" prefWidth="500.0">
        <Label id="labelQuestion" fx:id="labelQuestion" layoutX="34.0" layoutY="34.0" prefHeight="196.0"
               prefWidth="416.0"
               text="Question"/>
        <TextArea fx:id="textAreaAnswer" layoutX="113.0" layoutY="262.0" prefHeight="62.0" prefWidth="259.0"/>
        <Label fx:id="labelTEST" layoutX="229.0" layoutY="14.0" prefHeight="17.0" prefWidth="26.0" text="TEST"/>
        <Button fx:id="buttonNextQuestion" layoutX="170.0" layoutY="386.0" mnemonicParsing="false"
                onAction="#nextQuestionClick"
                prefHeight="42.0" prefWidth="151.0" text="Next"/>
    </AnchorPane>
</Group>

